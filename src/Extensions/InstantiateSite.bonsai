                <rx:Count>1</rx:Count>
              </Combinator>
            </Expression>
            <Expression xsi:type="rx:AsyncSubject">
              <Name>ThisSiteSpecs</Name>
            </Expression>
            <Expression xsi:type="SubscribeSubject">
              <Name>CurrentPosition</Name>
            </Expression>
            <Expression xsi:type="MemberSelector">
              <Selector>Z</Selector>
            </Expression>
            <Expression xsi:type="SubscribeSubject">
              <Name>ThisSiteSpecs</Name>
            </Expression>
            <Expression xsi:type="MemberSelector">
              <Selector>Start</Selector>
            </Expression>
            <Expression xsi:type="SubscribeSubject">
              <Name>ThisSiteSpecs</Name>
            </Expression>
            <Expression xsi:type="MemberSelector">
              <Selector>Start,Length</Selector>
            </Expression>
            <Expression xsi:type="Add" />
