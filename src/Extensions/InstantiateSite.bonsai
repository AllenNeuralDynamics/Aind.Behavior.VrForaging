                            <rx:Count>1</rx:Count>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="rx:AsyncSubject">
                          <Name>ThisRewardControl</Name>
                        </Expression>
                        <Expression xsi:type="SubscribeSubject">
                          <Name>IsStopped</Name>
                        </Expression>
