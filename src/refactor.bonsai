                    <gl:FramebufferAttachments />
                    <gl:VertexShader>BonVision:Shaders.TexturedModel.vert</gl:VertexShader>
                    <gl:FragmentShader>Shaders\ScaledTexturedModel.frag</gl:FragmentShader>
