{
  "$id": "h...",
  "$schema": "https://json-schema.org/draft-07/schema",
  "definitions": {
    "softwareEvent":{
      "type": "object",
      "properties": {
        "timestamp": {
          "type": ["null", "number"],
          "default": null
        },
        "timestampSource": {
          "type": "string",
          "enum": ["none", "harp", "render"],
          "default": "none"
        },
        "index": {
          "type": ["null","integer"],
          "minimum": null
        },
        "name":{
          "type": "string"
        },
        "data": {
          "type": ["null", "object"],
          "default": null
        },
        "dataType":{
          "type": "string",
          "enum": ["string", "number", "boolean", "object", "array", "null"],
          "default": null
        }
      }
    },
    "harpLogger":{
      "type": "object",
      "properties": {
        "logName":{
          "type":"string",
          "default": "harpDevice"
        },
        "extension":{
          "type":"string",
          "default": "bin"
        }
      }
    },
    "spinnakerLogger":{
      "type": "object",
      "properties": {
        "logName":{
          "type":"string",
          "default": "camera"
        },
        "encodingFrameRate":{
          "type":"number",
          "default": 60
        },
        "videoExtension":{
          "type":"string",
          "default": "avi"
        },
        "metadataExtension":{
          "type":"string",
          "default": "csv"
        },
        "codec": {
          "type": "string",
          "default": "FMP4"
        }
        }
      }
  },
  "properties": {
    "placeholder": {
      "type": "string"
    }
  },
  "title": "AindVrForagingLogging",
  "type": "object"
}
